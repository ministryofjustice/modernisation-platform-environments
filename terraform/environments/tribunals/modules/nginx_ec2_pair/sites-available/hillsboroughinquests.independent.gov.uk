server {
	listen 80;
	listen [::]:80;

	server_name hillsboroughinquests.independent.gov.uk;

	return  301 http://webarchive.nationalarchives.gov.uk/20170404105742/https://hillsboroughinquests.independent.gov.uk/$request_uri;
}
server {
        listen 80;
        listen [::]:80;

        server_name www.hillsboroughinquests.independent.gov.uk;

        return  301 http://webarchive.nationalarchives.gov.uk/20170404105742/https://hillsboroughinquests.independent.gov.uk/$request_uri;
}

