## REDIRECTOR SERVICE SETTINGS
server {
        listen 80;
        server_name www.hmica.gov.uk;
        location / {
                 return 301 http://webarchive.nationalarchives.gov.uk/20110206184228/http://www.hmica.gov.uk/;
        }
        access_log /var/log/nginx/hmica.gov.uk.log;
}
