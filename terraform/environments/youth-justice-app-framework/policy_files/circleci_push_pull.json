{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Action": [
              "ecr:BatchGetImage",
              "ecr:GetDownloadUrlForLayer",
              "ecr:ListImages",
              "ecr:DescribeImages",
              "ecr:PutImage",
              "ecr:InitiateLayerUpload"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:ecr:eu-west-2:${account_number}:yjaf/*"
      },
      {
          "Action": [
              "ecr:GetAuthorizationToken"
          ],
          "Effect": "Allow",
          "Resource": "*"
      }
  ]
}