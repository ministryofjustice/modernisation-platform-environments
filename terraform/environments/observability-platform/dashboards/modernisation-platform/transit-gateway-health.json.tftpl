{
  "id": null,
  "uid": "${dashboard_uid}",
  "title": "Transit Gateway Health - ${team_name} (${environment})",
  "tags": ["core-network-services", "networking", "transit-gateway"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "1m",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h", "6h"],
    "time_options": ["15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "region",
        "type": "query",
        "label": "AWS Region",
        "datasource": "core-network-services-production-cloudwatch",
        "query": "Regions",
        "refresh": 1,
        "includeAll": false,
        "multi": false,
        "current": {
          "text": "eu-west-2",
          "value": "eu-west-2",
          "selected": true
        }
      },
      {
        "name": "tgw_id",
        "type": "query",
        "label": "Transit Gateway",
        "datasource": "core-network-services-production-cloudwatch",
        "query": "dimension_values($region, AWS/TransitGateway, BytesIn, TransitGateway)",
        "refresh": 1,
        "includeAll": false,
        "multi": false
      },
      {
        "name": "tgw_attachment",
        "type": "query",
        "label": "Transit Gateway Attachment",
        "datasource": "core-network-services-production-cloudwatch",
        "query": "dimension_values($region, AWS/TransitGateway, BytesIn, TransitGatewayAttachment, {\"TransitGateway\": \"$tgw_id\"})",
        "refresh": 1,
        "includeAll": true,
        "allValue": "*",
        "multi": true
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Overall TGW Drop %",
      "description": "Dropped bytes (blackhole + no route) as % of total bytes for selected Transit Gateway",
      "datasource": "core-network-services-production-cloudwatch",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "options": {
        "reduceOptions": {
          "calcs": ["max"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.1 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesDropCountBlackhole",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id"
          },
          "period": "60"
        },
        {
          "refId": "B",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesDropCountNoRoute",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id"
          },
          "period": "60"
        },
        {
          "refId": "C",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesIn",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id"
          },
          "period": "60"
        },
        {
          "refId": "D",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesOut",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id"
          },
          "period": "60"
        },
        {
          "refId": "E",
          "expression": "(A + B) / (C + D) * 100"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "TGW Drop % Over Time",
      "description": "Trend of dropped bytes as % of total bytes for the selected Transit Gateway",
      "datasource": "core-network-services-production-cloudwatch",
      "gridPos": {
        "h": 6,
        "w": 16,
        "x": 8,
        "y": 0
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.1 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesDropCountBlackhole",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id"
          },
          "period": "60"
        },
        {
          "refId": "B",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesDropCountNoRoute",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id"
          },
          "period": "60"
        },
        {
          "refId": "C",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesIn",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id"
          },
          "period": "60"
        },
        {
          "refId": "D",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesOut",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id"
          },
          "period": "60"
        },
        {
          "refId": "E",
          "expression": "(A + B) / (C + D) * 100"
        }
      ]
    },
    {
      "id": 3,
      "type": "table",
      "title": "Dropped Bytes by Attachment",
      "description": "Per-attachment dropped bytes (blackhole + no route) for selected Transit Gateway",
      "datasource": "core-network-services-production-cloudwatch",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 6
      },
      "options": {
        "showHeader": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 1048576 },
              { "color": "red", "value": 10485760 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesDropCountBlackhole",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id",
            "TransitGatewayAttachment": "$tgw_attachment"
          },
          "period": "300",
          "hide": true
        },
        {
          "refId": "B",
          "region": "$region",
          "namespace": "AWS/TransitGateway",
          "metricName": "BytesDropCountNoRoute",
          "statistic": "Sum",
          "dimensions": {
            "TransitGateway": "$tgw_id",
            "TransitGatewayAttachment": "$tgw_attachment"
          },
          "period": "300",
          "hide": true
        },
        {
          "refId": "C",
          "expression": "A + B"
        }
      ]
    }
  ]
}
